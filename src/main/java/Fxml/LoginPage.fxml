<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Label?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewControllers.LoginPage">
    <TextField promptText="username" fx:id="username" GridPane.rowIndex="0"></TextField>
    <PasswordField promptText="password" fx:id="password" GridPane.rowIndex="1"></PasswordField>
    <Button text="confirm" GridPane.rowIndex="2" GridPane.columnSpan="2" onMouseClicked="#confirm"></Button>
    <Label fx:id="problem" text="" GridPane.rowIndex="3"/>
</GridPane>
