<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import java.net.URL?>
<GridPane  styleClass="StartPage" alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="ViewControllers.FirstPageController">
   <stylesheets>
      <URL value="@../Resources/styles.css"/>
   </stylesheets>
   <children >

      <Button styleClass="MyButton" GridPane.rowIndex="0" mnemonicParsing="false" text="start" onMouseClicked="#start"/>
      <Button styleClass="MyButton" GridPane.rowIndex="1" mnemonicParsing="false" text="end" onMouseClicked="#end" />
   </children>
</GridPane>
