<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../Resources/styles.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewControllers.BankWorksController">
   <children>
      <Button layoutX="220.0" layoutY="69.0" mnemonicParsing="false" onMouseClicked="#getToken" text="token" />
      <TextField fx:id="usernameForToken" layoutX="182.0" layoutY="120.0" promptText="username for Token" />
      <TextField fx:id="passwordForToken" layoutX="182.0" layoutY="175.0" promptText="password for Token" />
      <Label fx:id="problemOfToken" layoutX="242.0" layoutY="231.0" />
      <Button layoutX="475.0" layoutY="69.0" mnemonicParsing="false" onMouseClicked="#exit" text="exit" fx:id="exitButton" />
      <Button layoutX="663.0" layoutY="69.0" mnemonicParsing="false" onMouseDragged="#getTransaction" text="transaction" />
      <TextField fx:id="transactionToken" layoutX="647.0" layoutY="120.0" promptText="token" />
      <Label fx:id="resultOfTransaction" layoutX="707.0" layoutY="179.0" />
