<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10"  vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewControllers.ManagerRegisterController">
    <children>
        <TextField fx:id="username" text="userName" GridPane.rowIndex="0" />
        <TextField fx:id="password" text="password" GridPane.rowIndex="1" />
        <TextField fx:id="name" text="name" GridPane.rowIndex="2" />
        <TextField fx:id="lastName" text="lastName" GridPane.rowIndex="3" />
        <TextField fx:id="email" text="email" GridPane.rowIndex="4" />
        <TextField fx:id="phone" text="phoneNumber" GridPane.rowIndex="5" />
        <Button onMouseClicked="#confirm" text="confirm" GridPane.rowIndex="6" />
    </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
